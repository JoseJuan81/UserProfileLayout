{
	"name": "user-profile-layout",
	"version": "0.1.10",
	"main": "rollup/UserProfileLayout.umd.js",
	"module": "rollup/UserProfileLayout.esm.js",
	"unpkg": "rollup/UserProfileLayout.min.js",
	"browser": {
		"./sfc": "src/components/UserProfileLayout.vue"
	},
	"scripts": {
		"build": "npm run build:umd & npm run build:es & npm run build:unpkg",
		"build:es": "rollup --config build/rollup.config.js --format es --file rollup/UserProfileLayout.esm.js",
		"build:umd": "rollup --config build/rollup.config.js --format umd --file rollup/UserProfileLayout.umd.js",
		"build:unpkg": "rollup --config build/rollup.config.js --format iife --file rollup/UserProfileLayout.min.js",
		"cy:run": "cypress run",
		"dev": "vue-cli-service serve",
		"lint": "vue-cli-service lint",
		"prod": "vue-cli-service build",
		"test": "cypress open"
	},
	"dependencies": {
		"core-js": "^3.4.4",
		"vue": "^2.6.10"
	},
	"devDependencies": {
		"@rollup/plugin-buble": "^0.21.0",
		"@rollup/plugin-commonjs": "^11.0.1",
		"@vue/cli-plugin-babel": "^4.1.0",
		"@vue/cli-plugin-eslint": "^4.1.0",
		"@vue/cli-service": "^4.1.0",
		"@vue/eslint-config-airbnb": "^4.0.0",
		"babel-eslint": "^10.0.3",
		"cypress": "3.8.2",
		"eslint": "^5.16.0",
		"eslint-plugin-vue": "^5.0.0",
		"node-sass": "^4.13.1",
		"rollup-plugin-vue": "^5.1.5",
		"sass-loader": "^8.0.0",
		"vue-template-compiler": "^2.6.10"
	},
	"license": "MIT"
}
